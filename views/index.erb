
<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>Line</th>
      <th>File</th>
      <th>Timestamp</th>
            <th>Decoded</th>

    </tr>
  </thead>
  <tbody>
<% @reports.each do |report| %>
  <tr>
  <td><%= report.id %></td>
  <td><%= report.line %></td>
  <td><a href='<%= report.static_file_path %>'><%= report.static_file_path %></a></td>
  <td><%= report.timestamp %></td>
  <td><%= report.pretty_timestamp %></td>
  </tr>
<% end %>
</tbody>
</table>
